# =============================================
# SuiFund Production Environment Configuration
# =============================================

# Blockchain Configuration
VITE_CONTRACT_ADDRESS=0xYOUR_PRODUCTION_CONTRACT_ADDRESS
VITE_PACKAGE_ID=0xYOUR_PRODUCTION_PACKAGE_ID
VITE_SUI_NETWORK=mainnet
VITE_RPC_URL=https://fullnode.mainnet.sui.io:443

# API Configuration
VITE_API_BASE_URL=https://api.suifund.com
VITE_INDEXER_URL=https://indexer.suifund.com

# IPFS Configuration
VITE_IPFS_API_KEY=your_ipfs_api_key_production
VITE_IPFS_SECRET_API_KEY=your_ipfs_secret_key_production
VITE_IPFS_GATEWAY=https://ipfs.suifund.com

# Analytics & Monitoring
VITE_ANALYTICS_API_KEY=your_analytics_key_production
VITE_SENTRY_DSN=your_sentry_dsn_production
VITE_LOG_LEVEL=warn

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Security
VITE_APP_VERSION=1.0.0
VITE_BUILD_TIMESTAMP=2024-01-01T00:00:00Z