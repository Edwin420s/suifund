# Vite environment example for SuiFund web
# Copy this file to .env.local and fill in values per network

# Network: devnet | testnet | mainnet
VITE_SUI_NETWORK=devnet

# Package IDs per network (leave empty until deployed)
VITE_CAMPAIGN_PACKAGE_DEVNET=
VITE_PREDICTION_MARKET_PACKAGE_DEVNET=
VITE_NFT_PACKAGE_DEVNET=
VITE_TREASURY_PACKAGE_DEVNET=
VITE_UTILS_PACKAGE_DEVNET=

VITE_CAMPAIGN_PACKAGE_TESTNET=
VITE_PREDICTION_MARKET_PACKAGE_TESTNET=
VITE_NFT_PACKAGE_TESTNET=
VITE_TREASURY_PACKAGE_TESTNET=
VITE_UTILS_PACKAGE_TESTNET=

VITE_CAMPAIGN_PACKAGE_MAINNET=
VITE_PREDICTION_MARKET_PACKAGE_MAINNET=
VITE_NFT_PACKAGE_MAINNET=
VITE_TREASURY_PACKAGE_MAINNET=
VITE_UTILS_PACKAGE_MAINNET=

# API endpoints (optional)
VITE_GRAPHQL_ENDPOINT=
VITE_REST_API_ENDPOINT=
VITE_WS_ENDPOINT=

# Feature flags
VITE_ENABLE_PREDICTION_MARKETS=true
VITE_ENABLE_GOVERNANCE=true
VITE_ENABLE_NFT_REWARDS=true
VITE_ENABLE_SOCIAL_FEATURES=false
VITE_ENABLE_ANALYTICS=false
