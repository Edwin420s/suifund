# =============================================
# SuiFund Development Environment Configuration
# =============================================

# Blockchain Configuration
VITE_CONTRACT_ADDRESS=0xYOUR_DEV_CONTRACT_ADDRESS
VITE_PACKAGE_ID=0xYOUR_DEV_PACKAGE_ID
VITE_SUI_NETWORK=testnet
VITE_RPC_URL=https://fullnode.testnet.sui.io:443

# API Configuration
VITE_API_BASE_URL=http://localhost:3001
VITE_INDEXER_URL=http://localhost:3002

# IPFS Configuration
VITE_IPFS_API_KEY=your_ipfs_api_key_development
VITE_IPFS_SECRET_API_KEY=your_ipfs_secret_key_development
VITE_IPFS_GATEWAY=https://ipfs.io/ipfs

# Analytics & Monitoring
VITE_ANALYTICS_API_KEY=your_analytics_key_development
VITE_SENTRY_DSN=your_sentry_dsn_development
VITE_LOG_LEVEL=debug

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Security
VITE_APP_VERSION=1.0.0-dev
VITE_BUILD_TIMESTAMP=2024-01-01T00:00:00Z